{
  "emulators": {
    "firestore": {
      "host": "0.0.0.0",
      "port": "8080"
    },
    "hosting": {
      "host": "0.0.0.0"
    },
    "functions": {
      "host": "0.0.0.0"
    },
    "ui": {
      "host": "0.0.0.0"
    },
    "hub": {
      "host": "0.0.0.0"
    },
    "logging": {
      "host": "0.0.0.0"
    },
    "eventarc": {
      "host": "0.0.0.0"
    },
    "tasks": {
      "host": "0.0.0.0"
    }
  },
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions-js",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
